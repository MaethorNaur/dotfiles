{
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "dart",
    "scala",
    "js"
  ],
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.errors.incompleteClasspath.severity": "info",
  "metals.serverVersion": "0.10.0",
  "metals.statusBarEnabled": true,
  "metals.superMethodLensesEnabled": true,
  "metals.showInferredType": true,
  "metals.showImplicitArguments": true,
  "codeLens.enable": true,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "elixir.trace.server": "verbose",
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "/usr/local/opt/dart/libexec/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp",
        "--client-id",
        "vim",
        "--client-version",
        "coc.nvim"
      ],
      "filetypes": ["dart"],
      "initializationOptions": {
        "closingLabels": true,
        "suggestFromUnimportedLibraries": true
      },
      "progressOnInitialization": true
    },
    "golang": {
      "command": "gopls",
      "args": [],
      "initializationOptions": {
        "usePlaceholders": true
      },
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    },
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": [
        "terraform"
      ],
      "initializationOptions": {}
    }
  }
}
